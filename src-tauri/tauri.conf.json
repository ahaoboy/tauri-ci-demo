{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "tauri-ci-demo",
  "version": "0.1.0",
  "identifier": "com.tauri-ci-demo.app",
  "build": {
    "beforeDevCommand": "pnpm dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "tauri-ci-demo",
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "csp": null,
      "freezePrototype": true,
      "assetProtocol": {
        "enable": true,
        "scope": {
          "allow": [
            "**"
          ]
        }
      }
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "plugins": {
    "updater": {
      "windows": {
        "installMode": "passive"
      },
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IEU0QkUzNzlFMURBMDI2REIKUldUYkpxQWRuamUrNUFrRy8zWTg5ZXVxSkNFWjZvb0w1UEkvSlFSaEdSRzZlZzFjNm9qQjlPVS8K",
      "endpoints": [
        "https://github.com/ahaoboy/tauri-ci-demo/releases/latest/download/latest.json"
      ]
    }
  }
}